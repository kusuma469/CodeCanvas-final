{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/next"
        },
        {
            "src": "api/execute.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/execute",
            "dest": "api/execute.py",
            "methods": ["POST", "OPTIONS"]
        }
    ]
}