{
    "version": 2,
    "builds": [
      {
        "src": "app/api/execute/route.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb"
        }
      },
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/execute",
        "methods": ["POST", "OPTIONS"],
        "dest": "app/api/execute/route.py"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
}