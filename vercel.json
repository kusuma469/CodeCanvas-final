{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/next"
        },
        {
            "src": "app/api/execute/route.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "10mb",
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/execute",
            "dest": "app/api/execute/route.py",
            "methods": ["POST", "OPTIONS"]
        }
    ]
}